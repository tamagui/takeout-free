{
  "name": "takeout-free",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "files": [],
  "engines": {
    "node": "24.3.0",
    "npm": "10.8.3"
  },
  "packageManager": "bun@1.2.22",
  "scripts": {
    "android": "one run:android",
    "backend:clean": "docker compose down --volumes --remove-orphans --rmi local; rm -rf ./.docker/*",
    "backend": "bun run migrate:build && docker compose up --remove-orphans",
    "build": "one build",
    "check": "tko check",
    "check:all": "tko check --all",
    "postinstall": "bun run ./scripts/postinstall.ts",
    "ci": "NODE_ENV=production dotenvx run -f .env .env.production -- bun scripts/ci/release.ts",
    "clean": "rm -rf dist .expo .docker",
    "dev": "one dev",
    "env:dev": "NODE_ENV=development dotenvx run -f .env .env.development --",
    "env:test": "NODE_ENV=test dotenvx run -f .env .env.development --",
    "env:update": "tko run env-update",
    "ios": "one run:ios",
    "format:check": "oxfmt --check",
    "format": "oxfmt",
    "lint:fix": "oxfmt && oxlint --fix --fix-suggestions",
    "lint": "oxlint",
    "migrate": "bun run env:dev bun ./src/database/migrate.ts",
    "migrate:build": "tko migrate build",
    "prebuild:native": "one prebuild",
    "run:dev": "bun env:dev bun",
    "run:prod": "NODE_ENV=production dotenvx run -f .env .env.production -- bun",
    "run:test": "bun env:test bun",
    "test:integration": "bun run env:dev bun --cwd=./src/test playwright test",
    "test:unit": "bun run:dev vitest run -c ./src/test/vitest.config.ts",
    "test": "bun run test:unit && bun run test:integration",
    "zero:generate": "on-zero generate --after 'bun lint:fix'"
  },
  "overrides": {
    "@expo/fingerprint": "0.15.3"
  },
  "dependencies": {
    "@better-auth/expo": "^1.3.32",
    "@better-fetch/fetch": "^1.1.18",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@op-engineering/op-sqlite": "^15.0.7",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@rocicorp/zero": "0.25.11",
    "@take-out/better-auth-utils": "^0.0.67",
    "@take-out/cli": "^0.0.67",
    "@take-out/helpers": "^0.0.67",
    "@take-out/hooks": "^0.0.67",
    "@take-out/postgres": "^0.0.67",
    "@take-out/scripts": "^0.0.67",
    "@tamagui/animations-reanimated": "^2.0.0-rc.2",
    "@tamagui/config": "^2.0.0-rc.2",
    "@tamagui/constants": "^2.0.0-rc.2",
    "@tamagui/helpers-icon": "^2.0.0-rc.2",
    "@tamagui/linear-gradient": "^2.0.0-rc.2",
    "@tamagui/native": "^2.0.0-rc.2",
    "@tamagui/themes": "^2.0.0-rc.2",
    "@tamagui/toast": "^2.0.0-rc.2",
    "@tamagui/vite-plugin": "^2.0.0-rc.2",
    "@vxrn/color-scheme": "^1.4.10",
    "babel-plugin-react-compiler": "^1.0.0",
    "better-auth": "^1.3.32",
    "burnt": "^0.12.2",
    "drizzle-orm": "^0.40.0",
    "expo": "^54.0.22",
    "expo-blur": "~15.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-clipboard": "~8.0.7",
    "expo-crypto": "^15.0.7",
    "expo-dev-client": "~6.0.16",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linear-gradient": "~15.0.7",
    "expo-splash-screen": "^31.0.10",
    "expo-web-browser": "^15.0.9",
    "jose": "^6.0.11",
    "motion": "^12.29.0",
    "on-zero": "0.0.1",
    "one": "^1.4.10",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-bottom-tabs": "^0.10.0",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-keyboard-controller": "^1.20.2",
    "react-native-permissions": "^5.4.4",
    "react-native-reanimated": "^4.2.0",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "^4.19.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "^0.7.2",
    "tamagui": "^2.0.0-rc.2",
    "valibot": "^1.0.0",
    "vxrn": "^1.4.10"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.44.1",
    "@playwright/test": "^1.55.0",
    "@tamagui/cli": "^2.0.0-rc.2",
    "@types/async-retry": "^1.4.9",
    "@types/bun": "^1.2.16",
    "@types/node": "24.0.3",
    "@types/pg": "^8.11.10",
    "@types/react": "~19.1.10",
    "@types/react-dom": "~19.1.7",
    "drizzle-kit": "^0.30.5",
    "expo-plugin-ios-static-libraries": "^1.0.4",
    "oxfmt": "^0.16.0",
    "oxlint": "^1.31.0",
    "playwright": "^1.57.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.3"
  },
  "trustedDependencies": [
    "@rocicorp/zero-sqlite3"
  ],
  "env": {
    "BETTER_AUTH_SECRET": true,
    "BETTER_AUTH_URL": true,
    "ONE_SERVER_URL": true,
    "VITE_PUBLIC_ZERO_SERVER": "",
    "ZERO_VERSION": "0.25.11",
    "ZERO_NUM_SYNC_WORKERS": "2",
    "ZERO_CVR_MAX_CONNS": "4",
    "ZERO_UPSTREAM_MAX_CONNS": "8",
    "ZERO_PER_USER_MUTATION_LIMIT_MAX": "30",
    "ZERO_PER_USER_MUTATION_LIMIT_WINDOW_MS": "60000"
  },
  "publishConfig": {
    "access": "public"
  }
}
